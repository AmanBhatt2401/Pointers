#include<bits/stdc++.h>
using namespace std;

void swap(int *a,int *b){
    int temp=*a;
    *a=*b;
    *b=temp;
}

int main(){
    int a=10;
    int b=20;
    cout<<"Before Swap"<<endl;
    cout<<a<<" "<<b<<endl;
    
    int *aptr=&a;
    int *bptr=&b;
   
   swap(aptr,bptr);
   cout<<"After Swap:"<<endl;
   cout<<a<<" "<<b;
    return 0;
}
